[alias]
	co    = checkout
	ci    = commit
	st    = status
	br    = branch
	logs  = log --pretty=format:\"%h %ad | %s [%an]\" --graph --date=short
	type  = cat-file -t
	dump  = cat-file -p
[core]
	autocrlf = input
	safecrlf = false

[color]
	branch = auto
	diff = auto
	status = auto

[color "branch"]
	current = yellow reverse
	local = yellow
	remote = green

[color "diff"]
	meta = yellow bold
	frag = magenta bold
	old = red bold
	new = green bold

[color "status"]
	added = yellow
	changed = green
	untracked = cyan

[push]
	default = simple
[daemon]
	uploadarch = true

